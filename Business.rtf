\par }\pard \s3 \sb180\li180\fi-180\brdrl\brdrs\brdrw14\brsp43\brdrcf1\brdrt\brdrs\brdrw60\brsp43\brdrcf3\brdrr\brdrs\brdrw14\brsp43\brdrcf1\brdrb\brdrs\brdrw14\brsp43\brdrcf1 {\*\bkmkstart 703}{\*\bkmkend 703}{\b\fs32\cf2 SEC. 703.   TELEPHONE USERS TAX
